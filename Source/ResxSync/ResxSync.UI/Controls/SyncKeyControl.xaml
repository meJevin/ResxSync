<UserControl x:Class="ResxSync.UI.Controls.SyncKeyControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:ResxSync.UI.Controls"
             mc:Ignorable="d" Height="50" Width="185">
    <Grid Background="Aqua">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="30"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <TextBox x:Name="OwnersTB" IsReadOnly="True" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Padding="5">
                10
            </TextBox>
            <TextBox x:Name="KeyTB" IsReadOnly="True" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Padding="5" Grid.Column="1">
                Nigga
            </TextBox>
        </Grid>
    </Grid>
</UserControl>
